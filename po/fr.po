#: /home/georgesk/expeyes-web/expeyes-web/MicroHOPE/assembler.html:431
msgid ""
"\n"
"<code>        .section .text     ; denotes code section</code>\n"
"<code>        .global __vector_1 ; INT0_vect</code>\n"
"<code>__vector_1:                   ; Interrupt Service Routine of INT0. "
"Called when PD2 is LOW.</code>\n"
"<code>        INC R1</code>\n"
"<code>        OUT  0x18, R1</code>\n"
"<code>        RETI                  ; return from interrupt</code>\n"
"<code>        .global main</code>\n"
"<code>main:</code>\n"
"<code>        LDI  R16,  255</code>\n"
"<code>        OUT  0x17, R16        ; DDRB</code>\n"
"<code>        OUT  0x12, R16        ; enable Port D pull-up resistors</"
"code>\n"
"<code>        LDI  R16,  0x40       ; enable</code>\n"
"<code>        OUT  0x3b, r16        ; interrupt INT0</code>\n"
"<code>        CLR  R1</code>\n"
"<code>        SEI                   ; enable interrupts globally</code>\n"
"<code>loop:</code>\n"
"<code>        RJMP loop           ; infinite loop</code>\n"
"<code>        .end</code>\n"
msgstr ""
"\n"
"<code>        .section .text       ; dénote la section de code</code>\n"
"<code>        .global __vector_1   ; vecteur pour INT0</code>\n"
"<code>__vector_1:                  ; Sous-programme de service de "
"l'interruption INT0. Appelé quand PD2 est BAS.</code>\n"
"<code>        INC R1</code>\n"
"<code>        OUT  0x18, R1</code>\n"
"<code>        RETI                  ; retour de l'interruption</code>\n"
"<code>        .global main</code>\n"
"<code>main:</code>\n"
"<code>        LDI  R16,  255</code>\n"
"<code>        OUT  0x17, R16        ; DDRB</code>\n"
"<code>        OUT  0x12, R16        ; active les résistances de tirage du "
"port D</code>\n"
"<code>        LDI  R16,  0x40       ; activation</code>\n"
"<code>        OUT  0x3b, r16        ; interrupttion INT0</code>\n"
"<code>        CLR  R1</code>\n"
"<code>        SEI                   ; activation des interruptions "
"globallement</code>\n"
"<code>loop:</code>\n"
"<code>        RJMP loop             ; boucle infinie</code>\n"
"<code>        .end</code>\n"

